  "platform_customizations": {
    "custom_css": [
      ":root{--accent:#00d4ff;--ink:#0b0f14;--paper:#f7f8fb;--r:16px;--s:0 12px 28px rgba(0,0,0,.10)}",
      "body{color:var(--ink);background:var(--paper);-webkit-font-smoothing:antialiased}",
      "a,.link{color:var(--accent)}",
      ".header-wrapper{backdrop-filter:blur(10px);border-bottom:1px solid rgba(0,0,0,.06)}",
      ".announcement-bar{background:linear-gradient(90deg,rgba(0,212,255,.18),transparent)}",
      ".button,button,[type=submit]{border-radius:var(--r);font-weight:700;letter-spacing:.2px}",
      ".button--secondary{border-radius:var(--r)}",
      "input,select,textarea{border-radius:12px;border:1px solid rgba(0,0,0,.18)}",
      ".badge,.card__badge{border-radius:999px;font-weight:700}",
      ".card,.product-card-wrapper .card{border-radius:22px;overflow:hidden;box-shadow:var(--s);transition:transform .15s ease,box-shadow .15s ease}",
      ".card:hover{transform:translateY(-2px);box-shadow:0 18px 36px rgba(0,0,0,.14)}",
      ".card__heading,.product__title{letter-spacing:-.02em}",
      ".price{font-weight:800}",
      ".product__media-item img,.product__media img{border-radius:22px}",
      ".product-form__input input[type=radio]+label{border-radius:999px;border:1px solid rgba(0,0,0,.18);padding:.55rem .8rem}",
      ".product-form__input input[type=radio]:checked+label{border-color:var(--accent);box-shadow:0 0 0 2px rgba(0,212,255,.22)}",
      ".quantity__input{border-radius:12px}",
      ".facets__summary{border-radius:999px;border:1px solid rgba(0,0,0,.16);padding:.4rem .8rem}",
      "details{border:1px solid rgba(0,0,0,.10);border-radius:14px;padding:.25rem .9rem;background:#fff}"
